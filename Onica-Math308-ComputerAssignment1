    "T, Y = np.meshgrid(np.arange(0, 5, 0.25), np.arange(-3, 3, 0.25))\n",
    "dYdT = (np.sin(T)) * (np.cos(Y))  # put f(t, y) here to find slope\n",
    "U = 1 / (1 + dYdT**2)**0.5 * np.ones(T.shape)  # Normalizes the arrows to see near-zero slopes.\n",
    "V = 1 / (1 + dYdT**2)**0.5 * dYdT\n",
    "\n",
    "plt.figure()\n",
    "plt.title('Direction Field for ' + r'$\\frac{dy}{dt} = (\\sin{t})(\\cos{y})$')\n",
    "Q = plt.quiver(T, Y, U, V)  # draws the arrows at (X, Y) with slope dYdX"
